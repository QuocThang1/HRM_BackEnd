{
  "name": "backend-nodejs",
  "version": "1.0.0",
  "description": "Backend nodejs",
  "main": "server.js",
  "scripts": {
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon ./src/server.js",
