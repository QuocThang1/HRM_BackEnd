  },
  "keywords": [
    "nodejs",
    "react"
  ],
  "license": "ISC",
  "dependencies": {
    "bcrypt": "5.1.1",
    "cors": "2.8.5",
    "dotenv": "16.4.5",
    "ejs": "3.1.10",
    "express": "4.19.2",
    "jsonwebtoken": "9.0.2",
    "mongoose": "^8.4.4",
    "mongoose-sequence": "^6.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "nodemon": "3.1.4",
    "prettier": "^3.6.2"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "npm run format",
      "git add ."
